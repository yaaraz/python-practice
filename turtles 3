import turtle
wn = turtle.Screen()
wn.bgcolor("lightgreen")

wn.title("Hello, Tess!")

tess = turtle.Turtle()
tess.color("blue")
tess.pensize(3)

tess.forward(90)
tess.right(90)
tess.forward(90)
tess.right(90)
tess.forward(90)
tess.right(90)
tess.forward(90)
tess.right(90)

wn.mainloop()
           
